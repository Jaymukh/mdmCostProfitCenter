<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:form="sap.ui.layout.form" xmlns:l="sap.ui.layout">
	<VBox>
		<Table>
			<headerToolbar>
				<Toolbar>
					<ToolbarSpacer></ToolbarSpacer>
					<Button text="Add" type="Emphasized"/>
					<Button text="Edit" type="Emphasized"/>
					<Button icon="sap-icon://delete" type="Reject"/>
				</Toolbar>
			</headerToolbar>
			<columns>
				<Column >
					<header>
						<Label text="Actions" design="Bold"></Label>
					</header>
				</Column>
				<Column>
					<header>
						<Label text="Type" design="Bold"></Label>
					</header>
				</Column>
				<Column >
					<header>
						<Label text="Title" design="Bold"></Label>
					</header>
				</Column>
				<Column>
					<header>
						<Label text="Size" design="Bold"></Label>
					</header>
				</Column>
				<Column>
					<header>
						<Label text="Added On" design="Bold"></Label>
					</header>
				</Column>
				<Column>
					<header>
						<Label text="Added By" design="Bold"></Label>
					</header>
				</Column>
				<Column>
					<header>
						<Label text="Language" design="Bold"></Label>
					</header>
				</Column>
				<Column>
					<header>
						<Label text="Comment" design="Bold"></Label>
					</header>
				</Column>
			</columns>
			<items>
				<ColumnListItem>
					<cells></cells>
				</ColumnListItem>
			</items>
		</Table>
	</VBox>
</core:FragmentDefinition>