<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:form="sap.ui.layout.form" xmlns:l="sap.ui.layout">
	<form:SimpleForm editable="true" layout="ResponsiveGridLayout" labelSpanXL="4" labelSpanL="4" labelSpanM="4" labelSpanS="12"
		adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="3" columnsL="2" columnsM="2"
		singleContainerFullSize="false">
		<form:content>
			<core:Title text="Control Indicators"/>
			<Label text="Record Quantity"/>
			<CheckBox select="onSelectCheckBox" selected="{= ${CostCenter>/Csks/mgefl} === 'X'}" editable="{App>/edit}">
				<customData>
					<core:CustomData key="CostCenter>/Csks/mgefl"/>
				</customData>
			</CheckBox>
			<core:Title text=""/>
			<core:Title text="Lock Indicators"/>
			<Label text="Actual Primary Costs"/>
			<CheckBox select="onSelectCheckBox" selected="{= ${CostCenter>/Csks/bkzkp} === 'X'}" editable="{App>/edit}">
				<customData>
					<core:CustomData key="CostCenter>/Csks/bkzkp"/>
				</customData>
			</CheckBox>
			<Label text="Plan Primary Costs"/>
			<CheckBox select="onSelectCheckBox" selected="{= ${CostCenter>/Csks/pkzkp} === 'X'}" editable="{App>/edit}">
				<customData>
					<core:CustomData key="CostCenter>/Csks/pkzkp"/>
				</customData>
			</CheckBox>
			<Label text="Actual Revenues"/>
			<CheckBox select="onSelectCheckBox" selected="{= ${CostCenter>/Csks/bkzer} === 'X'}" editable="{App>/edit}">
				<customData>
					<core:CustomData key="CostCenter>/Csks/bkzer"/>
				</customData>
			</CheckBox>
			<Label text="Commitment Update"/>
			<CheckBox select="onSelectCheckBox" selected="{= ${CostCenter>/Csks/bkzob} === 'X'}" editable="{App>/edit}">
				<customData>
					<core:CustomData key="CostCenter>/Csks/bkzob"/>
				</customData>
			</CheckBox>
			<core:Title text=""/>
			<Label text="Actual Secondary Costs"/>
			<CheckBox select="onSelectCheckBox" selected="{= ${CostCenter>/Csks/bkzks} === 'X'}" editable="{App>/edit}">
				<customData>
					<core:CustomData key="CostCenter>/Csks/bkzks"/>
				</customData>
			</CheckBox>
			<Label text="Plan Secondary Costs"/>
			<CheckBox select="onSelectCheckBox" selected="{= ${CostCenter>/Csks/pkzks} === 'X'}" editable="{App>/edit}">
				<customData>
					<core:CustomData key="CostCenter>/Csks/pkzks"/>
				</customData>
			</CheckBox>
			<Label text="Plan Revenues"/>
			<CheckBox select="onSelectCheckBox" selected="{= ${CostCenter>/Csks/pkzer} === 'X'}" editable="{App>/edit}">
				<customData>
					<core:CustomData key="CostCenter>/Csks/pkzer"/>
				</customData>
			</CheckBox>
		</form:content>
